{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cash": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        }
      },
      "root": "projects/cash",
      "sourceRoot": "projects/cash/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/cash",
            "index": "projects/cash/src/index.html",
            "browser": "projects/cash/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/cash/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/cash/src/favicon.ico",
              "projects/cash/src/assets"
            ],
            "styles": [
              "projects/cash/src/styles.scss",
              "node_modules/ngx-toastr/toastr.css",
              "projects/cash/src/assets/fontawesome-free-6.4/scss/fontawesome.scss",
              "projects/cash/src/assets/fontawesome-free-6.4/scss/solid.scss",
              "projects/cash/src/assets/fontawesome-free-6.4/scss/regular.scss",
              "projects/cash/src/assets/fontawesome-free-6.4/scss/brands.scss",
              "projects/cash/src/assets/fontawesome-free-6.4/scss/v4-shims.scss"

            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/cash/src/environments/environment.ts",
                  "with": "projects/cash/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "cash:build:production"
            },
            "development": {
              "buildTarget": "cash:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "cash:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/cash/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/cash/src/favicon.ico",
              "projects/cash/src/assets"
            ],
            "styles": [
              "projects/cash/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
